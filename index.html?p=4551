	$('#testimonials-slider-4ddddc785e2f39fb06044a2b674de8b2').on('init', function(event, slick, currentSlide, nextSlide){
		$('#testimonials-4ddddc785e2f39fb06044a2b674de8b2 .sh-testimonials-switch').css( 'opacity','1' );
	});

	$('#testimonials-slider-4ddddc785e2f39fb06044a2b674de8b2').slick({
		dots: false,
		arrows: true,
		infinite: true,
		speed: 500,
		slidesToShow: 1,
		fade: true,
		responsive: [
			{
				breakpoint: 1000,
				settings: {
					slidesToShow: 1
				}
			}
		],
		adaptiveHeight: true,
		autoplay: false,
		autoplaySpeed: 5000,
	});

	function testimonials_slider_update_4ddddc785e2f39fb06044a2b674de8b2() {
		$('#testimonials-slider-4ddddc785e2f39fb06044a2b674de8b2 .slick-slide').css('height', '');
		var stHeight = $('#testimonials-slider-4ddddc785e2f39fb06044a2b674de8b2 .slick-track').height();
		$('#testimonials-slider-4ddddc785e2f39fb06044a2b674de8b2 .slick-slide').css('height',stHeight + 'px' );
	}

	testimonials_slider_update_4ddddc785e2f39fb06044a2b674de8b2();
	$(window).on( 'load resize', function() {
		setTimeout(function(){
			testimonials_slider_update_4ddddc785e2f39fb06044a2b674de8b2();
		}, 50);
	});

	$('#testimonials-4ddddc785e2f39fb06044a2b674de8b2 .sh-testimonials-prev').on( 'click', function() {
		$('#testimonials-slider-4ddddc785e2f39fb06044a2b674de8b2').slick('slickPrev');
	});

	$('#testimonials-4ddddc785e2f39fb06044a2b674de8b2 .sh-testimonials-next').on( 'click', function() {
		$('#testimonials-slider-4ddddc785e2f39fb06044a2b674de8b2').slick('slickNext');
	});

